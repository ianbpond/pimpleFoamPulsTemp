FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.031;

boundaryField
{
    bottomWall
    {
        type            fixedValue;
        value           uniform 1e-10;
    }
    frontAndBack
    {
        type            empty;
    }
    topWall
    {
        type            fixedValue;
        value           uniform 1e-10;
    }
    inout_half0
    {
        type            cyclic;
    }
    inout_half1
    {
        type            cyclic;
    }
}


