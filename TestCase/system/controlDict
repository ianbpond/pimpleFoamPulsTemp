FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoamPulsTempNew;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         900;

maxCo 1.0; // Or other Courant number you wish

adjustTimeStep no; // Or no

deltaT          0.0003;

writeControl    timeStep;

writeInterval   3125;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

